<%- model_class = Organizacion -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:razon_social) %>:</strong></dt>
  <dd><%= @organizacion.razon_social %></dd>
  <dt><strong><%= model_class.human_attribute_name(:entorno_id) %>:</strong></dt>
  <dd><%= @organizacion.entorno_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:interna_id) %>:</strong></dt>
  <dd><%= @organizacion.interna_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:externa_id) %>:</strong></dt>
  <dd><%= @organizacion.externa_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ambito_id) %>:</strong></dt>
  <dd><%= @organizacion.ambito_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:clase_id) %>:</strong></dt>
  <dd><%= @organizacion.clase_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              organizaciones_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_organizacion_path(@organizacion), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              organizacion_path(@organizacion),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

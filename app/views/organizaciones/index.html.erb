<%- model_class = Organizacion -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:nombre) %></th>
      <th><%= model_class.human_attribute_name(:descripcion) %></th>
      <th><%= model_class.human_attribute_name(:nombre_corto) %></th>
      <th><%= model_class.human_attribute_name(:rif) %></th>
      <th><%= model_class.human_attribute_name(:tolda_id) %></th>
      <th><%= model_class.human_attribute_name(:tipo_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @organizaciones.each do |organizacion| %>
      <tr>
        <td><%= link_to organizacion.id, organizacion_path(organizacion) %></td>
        <td><%= organizacion.nombre %></td>
        <td><%= organizacion.descripcion %></td>
        <td><%= organizacion.nombre_corto %></td>
        <td><%= organizacion.rif %></td>
        <td><%= organizacion.tolda_id %></td>
        <td><%= organizacion.tipo_id %></td>
        <td><%=l organizacion.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_organizacion_path(organizacion), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      organizacion_path(organizacion),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_organizacion_path,
            :class => 'btn btn-primary' %>

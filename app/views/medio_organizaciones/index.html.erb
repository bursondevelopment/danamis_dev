<%- model_class = MedioOrganizacion -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:organizacion_id) %></th>
      <th><%= model_class.human_attribute_name(:medio_id) %></th>
      <th><%= model_class.human_attribute_name(:propio) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @medio_organizaciones.each do |medio_organizacion| %>
      <tr>
        <td><%= link_to medio_organizacion.id, medio_organizacion_path(medio_organizacion) %></td>
        <td><%= medio_organizacion.organizacion_id %></td>
        <td><%= medio_organizacion.medio_id %></td>
        <td><%= medio_organizacion.propio %></td>
        <td><%=l medio_organizacion.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_medio_organizacion_path(medio_organizacion), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      medio_organizacion_path(medio_organizacion),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_medio_organizacion_path,
            :class => 'btn btn-primary' %>

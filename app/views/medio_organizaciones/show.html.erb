<%- model_class = MedioOrganizacion -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:organizacion_id) %>:</strong></dt>
  <dd><%= @medio_organizacion.organizacion_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:medio_id) %>:</strong></dt>
  <dd><%= @medio_organizacion.medio_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:propio) %>:</strong></dt>
  <dd><%= @medio_organizacion.propio %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              medio_organizaciones_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_medio_organizacion_path(@medio_organizacion), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              medio_organizacion_path(@medio_organizacion),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

<%= form_for @reporte_adjunto, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :reporte_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :reporte_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :adjunto_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :adjunto_id, :class => 'number_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                reportes_adjuntos_path, :class => 'btn' %>
  </div>
<% end %>

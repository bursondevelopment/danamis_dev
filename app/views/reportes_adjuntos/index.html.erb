<%- model_class = ReporteAdjunto -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:reporte_id) %></th>
      <th><%= model_class.human_attribute_name(:adjunto_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @reportes_adjuntos.each do |reporte_adjunto| %>
      <tr>
        <td><%= link_to reporte_adjunto.id, reporte_adjunto_path(reporte_adjunto) %></td>
        <td><%= reporte_adjunto.reporte_id %></td>
        <td><%= reporte_adjunto.adjunto_id %></td>
        <td><%=l reporte_adjunto.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_reporte_adjunto_path(reporte_adjunto), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      reporte_adjunto_path(reporte_adjunto),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_reporte_adjunto_path,
            :class => 'btn btn-primary' %>

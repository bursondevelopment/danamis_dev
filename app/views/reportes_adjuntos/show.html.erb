<%- model_class = ReporteAdjunto -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:reporte_id) %>:</strong></dt>
  <dd><%= @reporte_adjunto.reporte_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:adjunto_id) %>:</strong></dt>
  <dd><%= @reporte_adjunto.adjunto_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              reportes_adjuntos_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_reporte_adjunto_path(@reporte_adjunto), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              reporte_adjunto_path(@reporte_adjunto),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

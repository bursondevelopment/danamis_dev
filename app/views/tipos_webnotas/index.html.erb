<%- model_class = TipoWebnota -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:div_nota) %></th>
      <th><%= model_class.human_attribute_name(:div_titulo) %></th>
      <th><%= model_class.human_attribute_name(:div_fecha) %></th>
      <th><%= model_class.human_attribute_name(:div_contenido) %></th>
      <th><%= model_class.human_attribute_name(:div_imagen) %></th>
      <th><%= model_class.human_attribute_name(:website_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @tipos_webnotas.each do |tipo_webnota| %>
      <tr>
        <td><%= link_to tipo_webnota.id, tipo_webnota_path(tipo_webnota) %></td>
        <td><%= tipo_webnota.div_nota %></td>
        <td><%= tipo_webnota.div_titulo %></td>
        <td><%= tipo_webnota.div_fecha %></td>
        <td><%= tipo_webnota.div_contenido %></td>
        <td><%= tipo_webnota.div_imagen %></td>
        <td><%= tipo_webnota.website_id %></td>
        <td><%=l tipo_webnota.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_tipo_webnota_path(tipo_webnota), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      tipo_webnota_path(tipo_webnota),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_tipo_webnota_path,
            :class => 'btn btn-primary' %>

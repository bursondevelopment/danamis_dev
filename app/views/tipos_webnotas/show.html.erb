<%- model_class = TipoWebnota -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:div_nota) %>:</strong></dt>
  <dd><%= @tipo_webnota.div_nota %></dd>
  <dt><strong><%= model_class.human_attribute_name(:div_titulo) %>:</strong></dt>
  <dd><%= @tipo_webnota.div_titulo %></dd>
  <dt><strong><%= model_class.human_attribute_name(:div_fecha) %>:</strong></dt>
  <dd><%= @tipo_webnota.div_fecha %></dd>
  <dt><strong><%= model_class.human_attribute_name(:div_contenido) %>:</strong></dt>
  <dd><%= @tipo_webnota.div_contenido %></dd>
  <dt><strong><%= model_class.human_attribute_name(:div_imagen) %>:</strong></dt>
  <dd><%= @tipo_webnota.div_imagen %></dd>
  <dt><strong><%= model_class.human_attribute_name(:website_id) %>:</strong></dt>
  <dd><%= @tipo_webnota.website_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              tipos_webnotas_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_tipo_webnota_path(@tipo_webnota), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              tipo_webnota_path(@tipo_webnota),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
